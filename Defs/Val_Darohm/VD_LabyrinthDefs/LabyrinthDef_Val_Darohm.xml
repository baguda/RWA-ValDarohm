<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
	
	
	 <Adventure.PrimAdventure.DungeonDef>
	<!--Basic-->
		<defName>ValDarohmRuins</defName>
		<label>Ruins of Val'Darohm</label>
		
	<!--Labyrinth Gen-->
		<PathWidth>4</PathWidth>        
		<MapSize>(100,1,100)</MapSize>
        <WorldObjName>ValDarohmWO</WorldObjName>
		<IsLinked>false</IsLinked>
		<TargetBiome>Desert</TargetBiome>
        <StepsAway>50</StepsAway>
		<Enclosed>true</Enclosed>

	<!--Labyrinth Details-->	
        <!--isOutlined>false</isOutlined-->
		<isFilled>true</isFilled>
        <isWalled>true</isWalled>
        <isRoofed>true</isRoofed>
        <isShellRoofed>true</isShellRoofed>
        <isShellFloored>true</isShellFloored>
        <isCaveLike>false</isCaveLike>
		<UseBlockFloors>false</UseBlockFloors>
		
	<!--Enterance and exit-->
        <isWalkIn>false</isWalkIn>	
        <isInZDir>true</isInZDir>
        <isInPosDir>true</isInPosDir>
		
		<isWalkOut>false</isWalkOut>
        <isOutZDir>false</isOutZDir>
        <isOutPosDir>false</isOutPosDir>
		
        <isOutDoored>false</isOutDoored>
	
	<Settings>
	     <useWeather>true</useWeather> 
         <WeatherDefName>RPGGloom</WeatherDefName>
         <UseFoodFreeze>false</UseFoodFreeze> 
         <UseRestFreeze>false</UseRestFreeze>
         <UseJoyFreeze>false</UseJoyFreeze> 

         <UseNeedsModifiers>true</UseNeedsModifiers>
         <UseFoodModifier>true</UseFoodModifier>
         <UseRestModifier>true</UseRestModifier>
         <UseJoyModifier>true</UseJoyModifier>
         <TicksPerTurn>600</TicksPerTurn>
         <FoodPerTurn>0.025</FoodPerTurn>
         <NeedsPerTurn>0.01</NeedsPerTurn>
         <RestPerTurn>0.01</RestPerTurn>
         <JoyPerTurn>0.01</JoyPerTurn>

         <UseAdventureThought>true</UseAdventureThought>
         <UseAdventureHealth>true</UseAdventureHealth>
		 <HealthRegenPerTurn>0.1</HealthRegenPerTurn>
         <useParty>true</useParty>
         <PartySize>5</PartySize>
         <useCheckPoint>true</useCheckPoint>
         <UseDrafted>true</UseDrafted>
	</Settings>
	
	
	<!--DefNames PavedTile-->	
        <BlockDefName>RPGWall</BlockDefName>
        <FloorDefName>PavedTile</FloorDefName>
        <DoorDefName>Door</DoorDefName>
        <BlockStuffDefName>BlocksGranite</BlockStuffDefName>
        <FloorBlockDefName>Lava</FloorBlockDefName>
        <DoorStuffDefName>Steel</DoorStuffDefName>
		<EnterenceDefName>RPGStairsUp</EnterenceDefName>
		<ExitDefName></ExitDefName>
		<UseExit>false</UseExit>
        <encounterDefNames>

			<li>VD_MiniBoss</li>
			<li>VD_Tribe_PodC</li>
			<li>VD_Tribe_PodB</li>
			<li>VD_Tribe_PodA</li>
			<li>VD_Raiders_Podb</li>
			<li>VD_Raiders_PodA</li>
			<li>VD_Raiders_BasicB</li>
			<li>VD_Raiders_BasicA</li>
			<li>VD_Raiders_Basic</li>
		</encounterDefNames>
		<LinkedLabyrinthDefNames>
		<li></li>
		</LinkedLabyrinthDefNames>

	<!--Shapes-->			
